{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react","jsx-a11y"],
    "rules": {
      "semi": 2,
      "react/jsx-curly-brace-presence": 0,
      "react/no-array-index-key": 0,
      "react/jsx-curly-spacing": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "react/prop-types": 2
    },
    "globals": {
      "document": true,
      "__DEV__": true,
      "__PROD__": true
    }
  }